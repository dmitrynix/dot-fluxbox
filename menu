[begin] (Fluxbox-1.3.1)
  [encoding] {UTF-8}
    [exec] (Google Chrome) {/opt/google/chrome/chrome}
    [exec] (Nautilus) {nautilus --no-desktop}
    [exec] (xterm) {xterm}
    [exec] (Gnome Terminal) {gnome-terminal}
    [exec] (VirtualBox) {virtualbox}
    [exec] (Calibre) {calibre}
    [exec] (Backup with dejadup) {deja-dup --backup}
    [exec] (Screenshot - JPG) {import screenshot.jpg && display -resize 50% screenshot.jpg}
    [exec] (Screenshot - PNG) {import screenshot.png && display -resize 50% screenshot.png}
    [submenu] (System Styles ->) {Choose a style...}
      [stylesdir] (/usr/share/fluxbox/styles)
    [end]
    [submenu] (User Styles ->) {Choose a style...}
      [stylesdir] (~/.fluxbox/styles)
    [end]
    [submenu] (Games ->) {Choose a game...}
      [exec] (Steam) {/home/dmitry/Dropbox/Binary/steam.sh}
      [exec] (TF2) {/home/dmitry/Dropbox/Binary/tf2}
      [exec] (L4D2) {/home/dmitry/Dropbox/Binary/l4d2}
      [exec] (War Z) {/home/dmitry/Dropbox/Binary/warz}
    [end]
    [reconfig] (Reload config)
    [restart] (Restart)
    [exit] (Exit)
  [endencoding]
[end]
